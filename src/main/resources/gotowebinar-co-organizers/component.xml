<component name="gotowebinar-co-organizers" type="synapse/template">
    <subComponents>
        <component name="createCOOrganizer">
            <file>createCOOrganizer.xml</file>
            <description>Creates co-organizers for the specified webinar.</description>
        </component>
        <component name="listCOOrganizers">
            <file>listCOOrganizers.xml</file>
            <description>Returns the co-organizers for the specified webinar.</description>
        </component>
        <component name="deleteCOOrganizer">
            <file>deleteCOOrganizer.xml</file>
            <description>Deletes an internal co-organizer specified by the coorganizerKey (memberKey).</description>
        </component>
        <component name="resendInvitation">
            <file>resendInvitation.xml</file>
            <description>Resends an invitation email to the specified co-organizer.</description>
        </component>
    </subComponents>
</component>